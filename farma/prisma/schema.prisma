generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Catalog {
  id              Int     @id @default(autoincrement())
  title           String?
  descriptionList Json?   @db.Json
}

model Products {
  id          Int      @id @default(autoincrement())
  title       String?
  price       Int?
  description String?
  brand       String?
  order       Boolean?
}
